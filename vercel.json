{
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.7.1"
        }
    },
    "routes": [
      {
        "src": "/api/index.php",
        "dest": "/api/index.php"
      },
      {
        "src": "/style.css",
        "dest": "/public/style.css"
      },
      {
        "src": "/assets/img/(.*).jpeg",
        "dest": "/public/assets/img/first.jpeg"
      },
      {
        "src": "/assets/img/(.*).jpeg",
        "dest": "/public/assets/img/second.jpeg"
      },
      {
        "src": "/assets/datas/data.json",
        "dest": "/public/assets/datas/data.json"
      },
      {
        "src": "/(.*)",
        "dest": "/api/index.php"
      }
    ]
}